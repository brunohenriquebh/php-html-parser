{
    "name": "brunohenrique/php-html-parser",
    "type": "library",
    "version": "2.0.0",
    "description": "An HTML DOM parser. It allows you to manipulate HTML. Find tags on an HTML page with selectors just like jQuery.( new 2018/2019 PHP 7.2+ compatibility )",
    "keywords": ["html", "dom", "parser"],
    "homepage": "https://github.com/paquettg/php-html-parser",
    "license": "MIT",
    "authors": [
        {
            "name": "Bruno H. G. GOuvea",
            "email": "brunohenriquebh@gmail.com",
   
        }
    ],
    "require": {
        "php": ">=5.6",
        "paquettg/string-encode": "~0.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.3.0",
        "satooshi/php-coveralls": "~1.0.0",
        "mockery/mockery": "~0.9.0"
    },
    "autoload": {
        "psr-0": { 
        	"PHPHtmlParser": "src/"
       	}
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
